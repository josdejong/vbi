  },
  "license": "MIT",
  "devDependencies": {
    "ava": "0.13.0",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "copy-webpack-plugin": "1.1.1",
    "debug": "2.2.0",
    "eslint": "2.5.1",
    "eslint-plugin-react": "4.2.3",
    "fbjs": "0.2.1",
    "json-loader": "0.5.4",
    "lodash": "4.6.1",
    "material-ui": "0.15.0-alpha.2",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-hot-loader": "1.3.0",
